{
  "name": "node_barreiro",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "author": "Carlos Barreiro barreymata@gmail.com",
  "main": "src/app.ts",
  "scripts": {
    "start": "node bin/app.js",
    "start:watch": "nodemon -L -e ts,json --exec \"npm run compile\"",
    "dev": "npm run comileDev && bin/app.js",
    "dev:watch": "nodemon -L -e ts,json --exec \"npm run compileDev\"",
    "compile": "tsc && node bin/app.js",
    "compileDev": "tsc && node --inspect=0.0.0.0:9229 bin/app.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ci":  "yarn tsc"
  },
  "dependencies": {
    "@types/csvtojson": "^1.1.5",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "csvtojson": "^2.0.8",
    "ejs": "^2.5.7",
    "express": "^4.13.3",
    "express-fileupload": "^1.0.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "jwt-simple": "^0.5.1",
    "moment": "^2.19.1",
    "mongoose": "^4.13.0",
    "node-schedule": "^1.2.5",
    "nodemon": "^1.17.4",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/body-parser": "^1.16.7",
    "@types/express": "^4.0.37",
    "@types/express-fileupload": "^0.4.0",
    "@types/express-session": "^1.15.10",
    "@types/express-validator": "^3.0.0",
    "@types/jwt-simple": "^0.5.33",
    "@types/mongoose": "^4.7.36",
    "@types/node-schedule": "^1.2.1",
    "@types/passport": "^0.4.5",
    "@types/passport-google-oauth": "^1.0.38",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "typescript": "^2.0.3"
  },
  "license": "MIT"
}
